{
	"tabWidth": 2,
	"semi": true,
	"jsxSingleQuote": false,
	"bracketSpacing": true,
	"trailingComma": "all",
	"printWidth": 80,

	"importOrder": ["^(react/(.*)$)|^(react$)", "<THIRD_PARTY_MODULES>", "^[.]"],

	"plugins": [
		"@ianvs/prettier-plugin-sort-imports",
		"prettier-plugin-tailwindcss"
	]
}
